package pattern41;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		 int n=10;
			for(int i=0;i<=n;i++)
			{
		         for(int j=0;j<=n;j++)			
		         {
			        if(j==n/2||j==i &&j>=n/2||i>=n&&j<=n)
						{
						System.out.print("*");
						}
					else
					{
						System.out.print(" ");
					}
					}
					System.out.println();
				}


	}

}
