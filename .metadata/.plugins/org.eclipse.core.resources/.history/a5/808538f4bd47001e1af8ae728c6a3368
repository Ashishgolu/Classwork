package Pattern20;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 for (int l=1;l<=5;l++)
		  {
			  for(int m=5;m>=l;m--)
			  {
				  System.out.print("_");
			  }
			  for(int n=5;n>=l;n--)
			  {
				  
			  System.out.print("*");
	
			  }
			  
			  
			  
			  
			  
		     System.out.println();
		  }
	}  
	}
		  
	