package pattern41;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		 int n=10;
			for(int i=1;i<=n;i++)
			{
		         for(int j=1;j<=n;j++)			
		         {
			        if(j==n/2||j==i &&j>=n/2||j==n-1-i)
						{
						System.out.print("*");
						}
					else
					{
						System.out.print(" ");
					}
					}
					System.out.println();
				}


	}

}
